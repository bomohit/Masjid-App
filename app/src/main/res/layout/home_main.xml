<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/english_green">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/aclonica"
        android:text="MASJID JAMEK KEMUNTING"
        android:textAlignment="center"
        android:textColor="#FCFCFC"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/icon_home" />

    <Button
        android:id="@+id/buttonTazkirah"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/icon_tazkirah"
        app:layout_constraintEnd_toStartOf="@+id/buttonDonation"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:text="tazkirah" />

    <Button
        android:id="@+id/buttonDonation"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/icon_donation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@+id/buttonTazkirah"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:text="derma / donation" />

    <Button
        android:id="@+id/buttonBooking"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/icon_booking"
        app:layout_constraintEnd_toStartOf="@+id/buttonMayyitManagement"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonTazkirah"
        tools:text="tempah / Booking" />

    <Button
        android:id="@+id/buttonMayyitManagement"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/icon_mayyit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@+id/buttonBooking"
        app:layout_constraintTop_toBottomOf="@+id/buttonDonation"
        tools:text="Pengurusan Mayat" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tazkirah"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/buttonTazkirah"
        app:layout_constraintStart_toStartOf="@+id/buttonTazkirah"
        app:layout_constraintTop_toBottomOf="@+id/buttonTazkirah" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxWidth="60dp"
        android:text="Derma Donation"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/buttonDonation"
        app:layout_constraintStart_toStartOf="@+id/buttonDonation"
        app:layout_constraintTop_toBottomOf="@+id/buttonDonation" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxWidth="100dp"
        android:text="Tempah Booking"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/buttonBooking"
        app:layout_constraintStart_toStartOf="@+id/buttonBooking"
        app:layout_constraintTop_toBottomOf="@+id/buttonBooking" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxWidth="150dp"
        android:text="Pengurusan Mayat Mayyit Management"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/buttonMayyitManagement"
        app:layout_constraintStart_toStartOf="@+id/buttonMayyitManagement"
        app:layout_constraintTop_toBottomOf="@+id/buttonMayyitManagement" />
</androidx.constraintlayout.widget.ConstraintLayout>